
<%= render "layouts/errors", :object => @review %>

<%= form_for @review do |f| %>
<%= f.label :author %>
<%= f.text_field :author %>
<%= f.label :rating %>
<%= f.number_field :rating, step: 1, min: 1, max: 5 %>
<%= f.label :content_body %>
<%= f.text_area :content_body %>
<p><%= f.submit %></p>
<% end %>
